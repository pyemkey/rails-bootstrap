<% content_for :title do %>Rails Bootstrap<% end %>
<% content_for :description do %>Rails Bootstrap Example<% end %>
<div id="myCarousel" class="carousel slide" data-ride="carousel">
	<ol class="carousel-indicators">
		<li data-target="#myCarousel" data-slide-to="0" class="active"></li>
		<li data-target="#myCarousel" data-slide-to="1"></li>
		<li data-target="#myCarousel" data-slide-to="2"></li>
		<li data-target="#myCarousel" data-slide-to="3"></li>
	</ol>
	<div class="carousel-inner">
		<div class="item active">
			<img src="http://lorempixel.com/1170/600/city/9" alt="">
			<div class="carousel-caption text-center">
				<h4>San Francisco</h4>
			</div>
		</div>
		<div class="item">
			<img src="http://lorempixel.com/1170/600/city/7" alt="">
			<div class="carousel-caption text-center">
				<h4>Sydney</h4>
			</div>
		</div>
		<div class="item">
			<img src="http://lorempixel.com/1170/600/city/6" alt="">
			<div class="carousel-caption text-center">
				<h4>Paris</h4>
			</div>
		</div>
		<div class="item">
			<img src="http://lorempixel.com/1170/600/city/1" alt="">
			<div class="carousel-caption text-center">
				<h4>Berlin</h4>
			</div>
		</div>
	</div>
	<a href="#myCarousel" class="left carousel-control" data-slide="prev">&lsaquo;</a>
	<a href="#myCarousel" class="right carousel-control" data-slide="next">&rsaquo;</a>
</div>
